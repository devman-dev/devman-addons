<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="commission_app_view_tree" model="ir.ui.view">
        <field name="name">commission.app.view.tree</field>
        <field name="model">commission.app</field>
        <field name="arch" type="xml">
            <tree string="Comisión App">
                <field name="commission_rate" />
                <field name="date" />
            </tree>
        </field>
    </record>

    <record id="commission_app_view_form" model="ir.ui.view">
        <field name="name">commission.app.view.form</field>
        <field name="model">commission.app</field>
        <field name="arch" type="xml">
            <form string="Comisión App">
                <sheet>
                    <group>
                        <field name="commission_rate" />
                        <field name="date" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="commission_app_action" model="ir.actions.act_window">
        <field name="name">Comisión App</field>
        <field name="res_model">commission.app</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>